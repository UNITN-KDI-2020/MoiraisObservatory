    <!-- http://vocab.gtfs.org/terms#fareUrl -->

    <owl:AnnotationProperty rdf:about="http://vocab.gtfs.org/terms#fareUrl">
        <rdfs:comment xml:lang="es">La URL de una página web que permite a un usuario comprar online billetes u otros productos de transporte de esa empresa</rdfs:comment>
        <rdfs:comment xml:lang="en">The URL of a web page that allows a rider to purchase tickets or other fare instruments for that agency online</rdfs:comment>
        <rdfs:label xml:lang="es">URL de tarifas</rdfs:label>
        <rdfs:label xml:lang="en">fare URL</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://developers.google.com/transit/gtfs/reference#agency_fields"/>
