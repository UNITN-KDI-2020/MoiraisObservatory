    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#BeforeBoarding">
        <status:term_status xml:lang="en">unstable</status:term_status>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:comment xml:lang="es">La tarifa se debe pagar antes de subir a bordo</rdfs:comment>
        <rdfs:label xml:lang="en">Before Boarding</rdfs:label>
        <rdfs:label xml:lang="es">Antes de subir a bordo</rdfs:label>
        <rdfs:comment xml:lang="en">Ticket should be bought before boarding</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#Bus">
        <status:term_status xml:lang="en">unstable</status:term_status>
        <rdfs:comment xml:lang="en">Used for short- and long-distance bus routes.</rdfs:comment>
        <rdfs:label xml:lang="en">Bus</rdfs:label>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:comment xml:lang="es">Utilizado para rutas en autobús de corta y larga distancia.</rdfs:comment>
        <rdfs:label xml:lang="es">Autobús</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#CableCar">
        <status:term_status xml:lang="en">unstable</status:term_status>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:label xml:lang="en">Cable car</rdfs:label>
        <rdfs:comment xml:lang="en">Used for street-level cable cars where the cable runs beneath the car.</rdfs:comment>
        <rdfs:comment xml:lang="es">Utilizado para funiculares en superficie en donde el cable pasa por debajo del vehículo.</rdfs:comment>
        <rdfs:label xml:lang="es">Funicular</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#CheckParentStation">
        <comment xml:lang="es">Corresponde a wheelchair_accessible con valor 0.</comment>
        <comment xml:lang="en">Corresponds with wheelchair_accessible being 0.</comment>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:comment xml:lang="es">Compruebe la estación (matriz) para información de accesibilidad.</rdfs:comment>
        <rdfs:label xml:lang="es">Compruebe la estación matriz</rdfs:label>
        <rdfs:label xml:lang="en">Check the parent station</rdfs:label>
        <rdfs:comment xml:lang="en">Check the (parent) station for accessibility information.</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#EnsuredTransfer">
        <rdfs:comment xml:lang="es">Este es un punto de transbordo sincronizado entre dos rutas. El vehículo que sale espera al que llega, dejando tiempo suficiente para que un pasajero haga transbordo entre rutas.</rdfs:comment>
        <rdfs:label xml:lang="en">Ensured Transfer</rdfs:label>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <rdfs:label xml:lang="es">Punto de transbordo sincronizado</rdfs:label>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:comment xml:lang="en">This is a timed transfer point between two routes. The departing vehicle is expected to wait for the arriving one, with sufficient time for a passenger to transfer between routes.</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#Ferry">
        <rdfs:comment xml:lang="en">Used for short- and long-distance boat service.</rdfs:comment>
        <rdfs:comment xml:lang="es">Utilizado para servicio de transporte público fluvial o marítimo de corta y larga distancia.</rdfs:comment>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <rdfs:label xml:lang="en">Ferry</rdfs:label>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:label xml:lang="es">Ferry, transbordador</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#Funicular">
        <rdfs:comment xml:lang="es">Cualquier sistema por raíl diseñado para recorridos con una gran inclinación.</rdfs:comment>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:label xml:lang="en">Funicular</rdfs:label>
        <rdfs:label xml:lang="es">Funicular</rdfs:label>
        <rdfs:comment xml:lang="en">Any rail system designed for steep inclines.</rdfs:comment>
        <status:term_status xml:lang="en">unstable</status:term_status>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#Gondola">
        <rdfs:label xml:lang="es">Teleférico, telecabina</rdfs:label>
        <rdfs:comment xml:lang="en">Gondola, Suspended cable car. Typically used for aerial cable cars where the car is suspended from the cable.</rdfs:comment>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:comment xml:lang="es">Normalmente se utiliza para medios de transporte público por cable en donde el vehículo queda suspendido de un cable.</rdfs:comment>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <rdfs:label xml:lang="en">Gondola, Suspended cable car</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#LightRail">
        <rdfs:comment xml:lang="en">Any light rail or street level system within a metropolitan area.</rdfs:comment>
        <rdfs:comment xml:lang="es">Cualquier metro ligero o sistema ferroviario en superficie dentro de un área metropolitana.</rdfs:comment>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <rdfs:label xml:lang="en">Tram, Streetcar, Light rail</rdfs:label>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:label xml:lang="es">Tranvía, metro ligero</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#MinimumTimeTransfer">
        <rdfs:label xml:lang="es">Mínimo tiempo de transbordo</rdfs:label>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:label xml:lang="en">Minimum Time Transfer</rdfs:label>
        <rdfs:comment xml:lang="es">Este transbordo requiere una cantidad mínima de tiempo entre la llegada y la salida para garantizar la conexión. El tiempo necesario para el transbordo se especifica mediante min_transfer_time</rdfs:comment>
        <rdfs:comment xml:lang="en">This transfer requires a minimum amount of time between arrival and departure to ensure a connection. The time required to transfer is specified by gtfs:minimumTransferTime</rdfs:comment>
        <status:term_status xml:lang="en">unstable</status:term_status>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#MustCoordinateWithDriver">
        <rdfs:label xml:lang="en">Must coordinate with driver to arrange pickup or drop off</rdfs:label>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:label xml:lang="es">Se debe pedir al conductor que pare.</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#MustPhone">
        <rdfs:label xml:lang="es">Se debe llamar a la empresa para organizar la parada.</rdfs:label>
        <rdfs:label xml:lang="en">Must phone agency to arrange pickup or drop off</rdfs:label>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <status:term_status xml:lang="en">unstable</status:term_status>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#NoTransfer">
        <rdfs:label xml:lang="es">Sin transbordo</rdfs:label>
        <rdfs:comment xml:lang="es">No es posible realizar transbordos entre rutas en esta ubicación.</rdfs:comment>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <rdfs:label xml:lang="en">No Transfer</rdfs:label>
        <rdfs:comment xml:lang="en">Transfers are not possible between routes at this location.</rdfs:comment>
        <status:term_status xml:lang="es">inestable</status:term_status>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#NoTransfersAllowed">
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:comment xml:lang="es">Indica que el pasajero debe comprar un nuevo billete para realizar un transbordo</rdfs:comment>
        <rdfs:label xml:lang="en">No transfers allowed</rdfs:label>
        <rdfs:label xml:lang="es">No se permiten transbordos</rdfs:label>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <rdfs:comment xml:lang="en">Indicate that a passenger should buy a new ticket when transferring</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#NotAvailable">
        <status:term_status xml:lang="en">unstable</status:term_status>
        <rdfs:label xml:lang="en">Pickup or drop off not available</rdfs:label>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:label xml:lang="es">Sin parada disponible</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#NotWheelchairAccessible">
        <rdfs:label xml:lang="es">No accesible para silla de ruedas</rdfs:label>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <comment xml:lang="en">Corresponds to a wheelchair_accessible column being 2</comment>
        <rdfs:comment xml:lang="en">No riders in wheelchairs can be accommodated on this trip or wheelchair boarding is not possible at this stop and/or there exists no accessible path from outside the station to the specific stop.</rdfs:comment>
        <comment xml:lang="es">Corresponde a wheelchair_accessible con valor 2</comment>
        <rdfs:label xml:lang="en">Not wheelchair accessible</rdfs:label>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:comment xml:lang="es">Pasajeros en silla de ruedas no pueden acceder a este viaje o el acceso con silla de ruedas no es posible en esta parada y/o no hay una ruta accesible desde el exterior de la estación hasta la parada.</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#OnBoard">
        <status:term_status xml:lang="en">unstable</status:term_status>
        <rdfs:comment xml:lang="es">La tarifa se paga a bordo</rdfs:comment>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:label xml:lang="en">On Board</rdfs:label>
        <rdfs:label xml:lang="es">A bordo</rdfs:label>
        <rdfs:comment xml:lang="en">Fare should be bought on board</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#OneTransfersAllowed">
        <status:term_status xml:lang="en">unstable</status:term_status>
        <rdfs:label xml:lang="en">One transfer allowed</rdfs:label>
        <rdfs:comment xml:lang="en">Indicate that a passenger should buy a new ticket when transferring a second time</rdfs:comment>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:label xml:lang="es">Un transbordo permitido</rdfs:label>
        <rdfs:comment xml:lang="es">Indica que el pasajero puede hacer un transbordo con su billete</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#Rail">
        <rdfs:comment xml:lang="en">Used for intercity or long-distance travel.</rdfs:comment>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <rdfs:label xml:lang="en">Rail</rdfs:label>
        <rdfs:comment xml:lang="es">Utilizado para viajes de cercanías, interurbanos o de larga distancia.</rdfs:comment>
        <rdfs:label xml:lang="es">Tren</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#RecommendedTransfer">
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:comment xml:lang="en">This is a recommended transfer point between two routes</rdfs:comment>
        <rdfs:label xml:lang="en">Recommended Transfer</rdfs:label>
        <rdfs:label xml:lang="es">Punto de transbordo recomendado</rdfs:label>
        <rdfs:comment xml:lang="es">Este es un punto de transbordo recomendado entre dos rutas</rdfs:comment>
        <status:term_status xml:lang="en">unstable</status:term_status>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#Regular">
        <rdfs:label xml:lang="en">Regularly scheduled pickup or drop off</rdfs:label>
        <comment xml:lang="es">Si no se incluye pickup_type o drop_off_type en stop_times.txt, mapee a este concepto.</comment>
        <status:term_status xml:lang="en">unstable</status:term_status>
        <comment xml:lang="en">If no pickup_type or drop_off_type given in stop_times.txt, map to this one.</comment>
        <rdfs:label xml:lang="es">Parada programada regularmente</rdfs:label>
        <status:term_status xml:lang="es">inestable</status:term_status>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#Subway">
        <rdfs:comment xml:lang="es">Cualquier sistema ferroviario subterráneo dentro de un área metropolitana.</rdfs:comment>
        <rdfs:label xml:lang="en">Subway, Metro</rdfs:label>
        <status:term_status xml:lang="es">inestable</status:term_status>
        <rdfs:label xml:lang="es">Subterráneo, metro</rdfs:label>
        <rdfs:comment xml:lang="en">Any underground rail system within a metropolitan area.</rdfs:comment>
        <status:term_status xml:lang="en">unstable</status:term_status>
    </rdf:Description>
    <rdf:Description rdf:about="http://vocab.gtfs.org/terms#TwoTransfersAllowed">
